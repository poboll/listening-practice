{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": [
              ">0.3%",
              "not ie 11",
              "not dead",
              "not op_mini all"
            ]
          },
          "useBuiltIns": "usage",
          "corejs": 3
        }
      }
    ]
  ],
  "plugins": [
    [
      "transform-remove-console",
      {
        "exclude": [
          "error",
          "warn"
        ]
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    }
  }
}